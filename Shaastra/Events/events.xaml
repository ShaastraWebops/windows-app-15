    <!--LayoutRoot is the root grid where all page content is placed-->
    <Grid x:Name="LayoutRoot" Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        
        <StackPanel x:Name="TitlePanel" Grid.Row="0" Margin="12,17,0,28">
            <TextBlock Text="SHAASTRA" Foreground="White" Style="{StaticResource PhoneTextNormalStyle}" Margin="12,0"/>
            <TextBlock Text="events" Foreground="White" Margin="9,-7,0,0" Style="{StaticResource PhoneTextTitle1Style}"/>
        </StackPanel>

        <!--ContentPanel - place additional content here-->
        <Grid x:Name="ContentPanel" Grid.Row="1" Margin="12,12,0,12">
            <ScrollViewer>
                <StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/Aero.png" Height="90" x:Name="aero_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Aerofest" Foreground="Red" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="Aero_Button" Click="Aero_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/BE.png" Height="90" x:Name="be_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="B-Events" Foreground="Yellow" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="BE_Button" Click="BE_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/Code.png" Height="90" x:Name="code_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Coding" Foreground="Green" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="Code_Button" Click="Code_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/FS.png" Height="90" x:Name="fs_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Department Flagship" Foreground="Purple" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="FS_Button" Click="FS_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/DB.png" Height="90" x:Name="db_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Design and Build" Foreground="Orange" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="DB_Button" Click="DB_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/Elec.png" Height="90" x:Name="elec_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Electronics" Foreground="Blue" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="Elec_Button" Click="Elec_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/IQ.png" Height="90" x:Name="inv_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Involve" Foreground="Turquoise" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="IQ_Button" Click="IQ_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Assets/IQ.png" Height="90" x:Name="qu_icon" Width="90" Stretch="Uniform" Opacity="1"/>
                        <Button Content="Quizzing" Foreground="Brown" FontSize="36" BorderThickness="0" Background="Transparent" FontFamily="Comic Sans MS" x:Name="Shows_Button" Click="Shows_Button_Click" Height="90">
                        </Button>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
