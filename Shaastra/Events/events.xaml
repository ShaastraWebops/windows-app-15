                    <Button Content="Aerofest" Foreground="Black" FontSize="36" BorderThickness="0" Background="Red" FontFamily="Jokerman" x:Name="Aero_Button" Click="Aero_Button_Click" Height="90">
                    </Button>
                    <Button Content="Design and Build" Foreground="Black" FontSize="36" BorderThickness="0" Background="Orange" FontFamily="Jokerman" x:Name="DB_Button" Click="DB_Button_Click" Height="90">
                    </Button>
                    <Button Content="Coding" Foreground="Black" FontSize="36" BorderThickness="0" Background="Green" FontFamily="Jokerman" x:Name="Code_Button" Click="Code_Button_Click" Height="90">
                    </Button>
                    <Button Content="Involve and Quizzes" Foreground="Black" FontSize="36" BorderThickness="0" Background="Turquoise" FontFamily="Jokerman" x:Name="IQ_Button" Click="IQ_Button_Click" Height="90">
                    </Button>
                    <Button Content="Electronics" Foreground="Black" FontSize="36" BorderThickness="0" Background="Blue" FontFamily="Jokerman" x:Name="Elec_Button" Click="Elec_Button_Click" Height="90">
                    </Button>
                    <Button Content="Flagship" Foreground="Black" FontSize="36" BorderThickness="0" Background="Purple" FontFamily="Jokerman" x:Name="FS_Button" Click="FS_Button_Click" Height="90">
                    </Button>
                    <Button Content="Spotlight" Foreground="Black" FontSize="36" BorderThickness="0" Background="Lime" FontFamily="Jokerman" x:Name="SL_Button" Click="SL_Button_Click" Height="90">
                    </Button>
                    <Button Content="Workshop" Foreground="Black" FontSize="36" BorderThickness="0" Background="Gold" FontFamily="Jokerman" x:Name="WS_Button" Click="WS_Button_Click" Height="90">
                    </Button>
                    <Button Content="Shows" Foreground="Black" FontSize="36" BorderThickness="0" Background="Brown" FontFamily="Jokerman" x:Name="Shows_Button" Click="Shows_Button_Click" Height="90">
                    </Button>
                    <Button Content="B-Events" Foreground="Black" FontSize="36" BorderThickness="0" Background="Yellow" FontFamily="Jokerman" x:Name="BE_Button" Click="BE_Button_Click" Height="90">
                    </Button>
                </StackPanel>
            </ScrollViewer>
            <Button x:Name="Back_Button" BorderThickness="0" Click="Back_Button_Click" Background="Crimson" Opacity="0" Height="90" Width="90" Margin="12,500,370,12"/>
            <Image x:Name="Aero_img" Source="Assets/Aero.png" Opacity="0" Height="100" Width="100" Margin="10,10,359,502">
